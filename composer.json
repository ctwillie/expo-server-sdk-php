{
    "name": "ctwillie/expo-server-sdk-php",
    "description": "Server-side library for working with Expo using PHP",
    "keywords": [
        "expo",
        "php",
        "expo-server-sdk-php"
    ],
    "homepage": "https://github.com/ctwillie/expo-server-sdk-php",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/ctwillie/expo-server-sdk-php/issues",
        "source": "https://github.com/ctwillie/expo-server-sdk-php"
    },
    "authors": [
        {
            "name": "Cedric Twillie",
            "email": "ctwillie77@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.3",
        "guzzlehttp/guzzle": "^6|^7",
        "ext-json": "*",
        "ext-zlib": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "ExpoSDK\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ExpoSDK\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit --testdox",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
